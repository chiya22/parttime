<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1><%= title %></h1>
  <ul>
    <% if (users) { %>
    <%  users.forEach( user => { %>
    <li>
      <a href="users/update/<%= user.id %>">
        <%= user.name %>（<%= user.id %>）
      </a>
    </li>
    <%  }) %>
    <% } %>
  </ul>
  <a href="users/insert">登録</a>
</body>

</html>